{
  "name": "scrapedpogo",
  "version": "1.0.0",
  "description": "Scrape LeekDuck.com for relevant information about Pokemon GO",
  "main": "index.js",
  "scripts": {
    "scrape": "node ./src/scrapers/scrape.js",
    "detailedscrape": "node ./src/scrapers/detailedscrape.js",
    "combinedetails": "node ./src/scrapers/combinedetails.js",
    "scrapeshinies": "node ./src/scrapers/scrapeShinies.js",
    "pipeline": "npm run scrapeshinies && npm run scrape && npm run detailedscrape && npm run combinedetails",
    "validate": "node scripts/validate-schemas.js",
    "imagesize": "node scripts/calculate-image-size.js",
    "blob:upload": "node scripts/upload-images-to-blob.js",
    "blob:upload:dry": "node scripts/upload-images-to-blob.js --dry-run",
    "blob:upload:force": "node scripts/upload-images-to-blob.js --force",
    "blob:transform": "node scripts/transform-urls-to-blob.js",
    "blob:transform:dry": "node scripts/transform-urls-to-blob.js --dry-run",
    "ai:enrich-events": "node scripts/enrich-events-ai.js"
  },
  "author": "quantNebula",
  "license": "MIT",
  "dependencies": {
    "@vercel/blob": "^2.0.1",
    "image-size": "^2.0.2",
    "jsdom": "27.4.0",
    "moment": "latest"
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  }
}
